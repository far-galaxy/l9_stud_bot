services:
  bot:
    image: git.arai.icu/arairon/l9_stud_bot
    #build:
    #  context: ./source
    #  dockerfile: ./Dockerfile
    container_name: l9_stud_bot
    restart: always
    volumes:
      - ./logs:/app/logs
      - ./schedules:/app/shedules
    ports:
      - 8000:8000
    env_file: .env
    depends_on:
      - db
  db:
    image: mysql
    restart: always
    volumes:
      - ./db:/var/lib/mysql
    env_file: .env
networks: {}
